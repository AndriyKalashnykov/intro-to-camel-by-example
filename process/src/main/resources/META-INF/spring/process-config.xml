<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">
  
  <util:properties id="defaultProperties">
    <prop key="example.jug.camel.process.test.alternatePersistUri">-=NULL=-</prop>
    <prop key="example.jug.camel.process.records.queue.name">example.jug.camel.ingest.records.raw</prop>
    <prop key="example.jug.camel.process.maxConcurrentConsumers">1</prop>
    <prop key="example.jug.camel.process.error.queue.name">ActiveMQ.DLQ</prop>
    
    <prop key="example.jug.camel.jdbc.driver.class">com.mysql.jdbc.Driver</prop>
    <prop key="example.jug.camel.jdbc.url"></prop>
    <prop key="example.jug.camel.jdbc.username">-=NULL=-</prop>
    <prop key="example.jug.camel.jdbc.password">-=NULL=-</prop>
    <prop key="example.jug.camel.jdbc.pool.min">10</prop>
    <prop key="example.jug.camel.jdbc.pool.max">50</prop>
    <prop key="example.jug.camel.jdbc.validation.query"></prop>
    <prop key="example.jug.camel.jdbc.validation.active">false</prop>
    
    <prop key="example.jug.camel.jms.maxConnections">5</prop>
    <prop key="example.jug.camel.jms.maximumActive">500</prop>
    <prop key="example.jug.camel.jms.user">-=NULL=-</prop>
    <prop key="example.jug.camel.jms.password">-=NULL=-</prop>
    <prop key="example.jug.camel.jms.brokerUrl"></prop>
    <prop key="example.jug.camel.jms.initialRedeliveryDelay">1000</prop>
    <prop key="example.jug.camel.jms.maximumRedeliveries">6</prop>
  </util:properties>
</beans>