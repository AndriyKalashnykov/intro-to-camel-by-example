<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:camel="http://camel.apache.org/schema/spring"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
           http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">
           
  <import resource="classpath:META-INF/spring/ingest-core.xml"/>
  <import resource="classpath:META-INF/spring/ingest-jms.xml"/>
  <import resource="classpath:META-INF/spring/ingest-config.xml"/>
  
  <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
    <property name="propertiesArray">
      <list>
        <ref bean="defaultProperties"/>
	    <props>
	      <prop key="example.jug.camel.ingest.source.dir">./target/test-data/source</prop>
	      <prop key="example.jug.camel.jms.brokerUrl">vm://localhost?broker.persistent=false</prop>
	    </props>
      </list>
    </property>
  </bean>
</beans>
